#include<iostream>
#include <queue>
using namespace std;
struct stack
{
    queue<int> q1,q2;
    void push(int val)
    {
        while (!q1.empty())
        {
            q2.push(q1.front());
            q1.pop();
        }
        q1.push(val);
        while(!q2.empty())
        {
            q1.push(q2.front());
            q2.pop();
        }
    }
    int pop()
    {
        int x=q1.front();
        q1.pop();
        return x;
    }
    void print()
    {
        while(!q1.empty())
        {
            int x=q1.front();
            cout<<x<<" ";
            q1.pop();
        }
        cout<<endl;
    }
    int top()
    {
        int x=q1.front();
        return x;
    }
};
int main()
{
    stack s;
    s.push(1);
    s.push(2);
    s.push(3);
    cout<<s.top()<<" ";
    s.pop();
    s.print();
    cout<<endl;
    return 0;
}
